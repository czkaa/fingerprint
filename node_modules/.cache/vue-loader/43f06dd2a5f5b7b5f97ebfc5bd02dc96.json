tyle=\"{\n      filter: 'drop-shadow(0px 0px 10px white) blur(' + blur + ') contrast(3) saturate(0.4) brightness(1.1)',\n    }\">\n      <div\n      :style=\"{\n        height: '90%',\n        width: '90%',\n        left: '5%',\n        top: '7%',\n        background: 'linear-gradient(' + fingerprint.hex[4] + ', ' + fingerprint.hex[2] + ', ' + fingerprint.hex[3] + ', ' + fingerprint.hex[1] + ')',\n        borderRadius: '100%',\n        transform: 'perspective(5000px) rotateY(' + xPercent  + 'deg) rotateX(' + yPercent *1.2 + 'deg) rotateZ(' + yPercent * 0.5 + 'deg)'\n        }\"\n      :class=\"[\n        'z-10',\n        'absolute',\n        'transition-all duration-100',\n        'mix-blend-exclusion'\n      ]\"/>\n      <div\n      :style=\"{\n        height: '70%',\n        width: '70%',\n        left: '15%',\n        top: '15%',\n        background: 'linear-gradient(' + fingerprint.hex[0] + ', ' + fingerprint.hex[2] + ', ' + fingerprint.hex[3] + ')',\n        borderRadius: '100%',\n        transform: 'perspective(5000px) rotateY(' + xPercent * this.fingerprint.int[5] + 'deg) rotateX(' + yPercent + 'deg) rotateZ(' + yPercent * 1/ this.fingerprint.int[5] + 'deg)'\n        }\"\n        :class=\"[\n          'z-10',\n          'absolute',\n          'transition-all duration-100',\n          'mix-blend-exclusion'\n        ]\"/>\n\n        <div\n        :style=\"{\n          height: '70%',\n          width: '70%',\n          left: '15%',\n          top: '5%',\n          background: 'linear-gradient(' + fingerprint.hex[2] + ', ' + fingerprint.hex[0] + ', ' + fingerprint.hex[1] + ')',\n          borderRadius: '100%',\n          transform: 'perspective(5000px) rotateY(' + xPercent * fingerprint.int[2] + 'deg) rotateX(' + yPercent + 'deg) rotateZ(' + yPercent * 1/ fingerprint.int[4] + 'deg)'\n          }\"\n          :class=\"[\n            'z-10',\n            'absolute',\n            'transition-all duration-100',\n            'mix-blend-exclusion'\n          ]\"/>\n\n\n      </div>\n\n    </div>\n\n\n  </div>\n\n\n\n</template>\n\n<script>\n\n  export default {\n    name: 'SnippetFingerprint',\n    components: {\n    },\n    data() {\n      return {\n        y: 0,\n        x: 0,\n        yPercent: Math.floor(Math.random() * (30 - 0)) + 0,\n        xPercent:  Math.floor(Math.random() * (30 - 0)) + 0,\n        viewportHeight: 1,\n        viewportWidth: 1\n      }\n    },\n    props: {\n      fingerprint: Object,\n      blur: String,\n      autorotate: {\n        type: Boolean,\n        default: false\n      }\n    },\n    methods: {\n      onMouseMove (ev) {\n        this.x = ev.clientX\n        this.y = ev.clientY\n        this.xPercent = this.x/this.viewportWidth * 60\n        this.yPercent = this.y/this.viewportHeight * 60\n      },\n\n      updateViewportSize () {\n        this.viewportHeight = window.innerHeight\n        this.viewportWidth = window.innerWidth\n      },\n\n      rotate() {\n        let yIncrement = true\n        let xIncrement = true\n        setInterval(()=>{\n          if (this.yPercent < 60 && yIncrement) {\n            this.yPercent ++\n          } else if (this.yPercent < 1) {\n            yIncrement = true\n          } else {\n            yIncrement = false\n            this.yPercent--\n          }\n\n          if (this.xPercent < 60 && xIncrement) {\n            this.xPercent ++\n          } else if (this.xPercent < 1) {\n            xIncrement = true\n          } else {\n            xIncrement = false\n            this.xPercent--\n          }\n        }, 100)\n      }\n    },\n    created () {\n      this.updateViewportSize()\n    },\n    unmounted () {\n      document.removeEventListener('mousemove', this.onMouseMove)\n      window.removeEventListener('resize', this.updateViewportSize)\n    },\n    mounted() {\n      if (!this.autorotate) {\n        document.addEventListener('mousemove', this.onMouseMove)\n        window.addEventListener('resize', this.updateViewportSize)\n      }\n      this.rotate()\n\n    }\n  }\n\n</script>\n\n<style lang=\"scss\">\n\n</style>\n"]}]}